{
  "name": "iosapp2",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "npm run lint --workspace=@inverter/mobile",
    "test": "npm run test --workspace=@inverter/mobile",
    "typecheck": "npm run typecheck --workspace=@inverter/mobile",
    "expo-doctor": "npm run expo-doctor --workspace=@inverter/mobile",
    "detox:build": "npm run detox:build --workspace=@inverter/mobile",
    "detox:test": "npm run detox:test --workspace=@inverter/mobile",
    "dev:check": "node scripts/dev/ensure-api.js",
    "dev:start": "node scripts/dev/start-expo.js",
    "dev:ios": "EXPO_START_MODE=local EXPO_LAUNCH_SIMULATOR=true npm run dev:start",
    "dev:lan": "EXPO_START_MODE=lan npm run dev:start",
    "dev:tunnel": "EXPO_START_MODE=tunnel EXPO_LAUNCH_SIMULATOR=false npm run dev:start",
    "api:web": "cd ../inverter-app-api-start && make web"
  }
}
